{
  "name": "b2io-shark",
  "version": "1.0",
  "description": "An extension to facilitate creating Grooveshark playlists.",
  "background": {
    "scripts": [ "js/background.js" ]
  },
  "page_action": {
    "default_icon": "img/icon-19.png",
    "default_popup": "popup.html",
    "default_title": "b2io-shark"
  },
  "content_scripts": [
    {
      "matches": [
        "http://grooveshark.com/*"
      ],
      "js": [
        "js/jquery.min.js",
        "js/GroovesharkProxy.js",
        "js/grooveshark.js",
        "js/lastfm.js",
        "js/hypem.js",
        "js/reddit.js",
        "js/pitchfork.js",
        "js/xml2json.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "manifest_version": 2
}